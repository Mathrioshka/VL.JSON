<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/10/nuspec.xsd">
  <metadata>
    <id>VL.JSON</id>
    <version>0.0.3</version>
    <authors>smakhtin</authors>
    <projectUrl>https://github.com/mathrioshka/VL.JSON</projectUrl>
